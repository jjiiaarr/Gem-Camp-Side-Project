<h1>Admin::UserList#index</h1>
<p>Find me in app/views/admin/user_list/index.html.erb</p>

<h1>Admin Users Lists</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>parent email</td>
  <td>email</td>
  <td>total_deposit</td>
  <td>member_total_deposits</td>
  <td>coins</td>
  <td>total used coins</td>
  <td>children members</td>
  <td>phone number</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.parent&.email %></td>
      <td><%= user.email %></td>
      <td><%= user.total_deposit %></td>
      <td><%= user.children.sum(:total_deposit) %></td>
      <td><%= user.coins %></td>
      <td><%= user.coins %></td>
      <td><%= user.children_members %></td>
      <td><%= user.phone_number %></td>
    </tr>
  <% end %>
</table>