<%= form_with model: category, url: path, method: method, data: { turbo: false } do |f| %>
  <div class="col-md-4">
  <%= f.label :name, class: "form-label" %>
  <%= f.text_field :name, class: "form-control form-control-lg" %>
  <%= f.submit "Submit", class: "btn btn-primary btn-block" %>
  </div>
<% end %>