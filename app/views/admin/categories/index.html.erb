<h1>Admin::Categories#index</h1>
<p>Find me in app/views/admin/categories/index.html.erb</p>

<h1>Category</h1>

<ul>
  <% @categories.each do |category| %>
    <li class="list-unstyled">
      <%= category.name %>
      <%= link_to :edit, edit_admin_category_path(category) %>
      <%= button_to :delete, admin_category_path(category), method: :delete, data: {turbo: false} %>
    </li>
  <% end %>
</ul>

<%= link_to 'new', new_admin_category_path %>