<h1>Admin Users Lists</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>parent email</td>
  <td>email</td>
  <td>total_deposit</td>
  <td>member_total_deposits</td>
  <td>coins</td>
  <td>total used coins</td>
  <td>children members</td>
  <td>phone number</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>