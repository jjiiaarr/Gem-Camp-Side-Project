<h1>Admin Users</h1> <br/>

<table class="table table-striped table-hover">
  <thead>
  <td>id</td>
  <td>email</td>
  <td>username</td>
  <td>genre</td>
  <td>phone number</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.username %></td>
      <td><%= user.genre %></td>
      <td><%= user.phone_number %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Sign out', destroy_user_session_path, data: { 'turbo-method': :delete } %>